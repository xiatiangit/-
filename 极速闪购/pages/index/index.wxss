.lunbotu swiper {
  width: 750rpx;
  height: 340rpx;
}
.lunbotu swiper image {
  width: 100%;
}
.daohang {
  display: flex;
}
.daohang navigator {
  flex: 1;
  padding: 20rpx;
}
.daohang navigator image {
  width: 100%;
}
.louceng .floor .title {
  padding: 10rpx 0;
}
.louceng .floor .title image {
  width: 100%;
}
.louceng .floor .content {
  float: left;
  width: 33.33%;
}
.louceng .floor .content:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 5px solid #fff;
}
.louceng .floor .content:nth-child(4),
.louceng .floor .content:nth-child(3) {
  border-bottom: 5px solid #fff;
}
.louceng .floor .content image {
  width: 100%;
  height: 100%;
}
